{
    "build": {
      "cpu": "cortex-m0plus",
      "extra_flags": "-DSTM32L052xx",
      "f_cpu": "32000000L",
      "mcu": "stm32l052k8u6",
      "product_line": "STM32L052xx",
      "platform": "stm32",
      "variant": "AWOHALI_STM32L052K8",
      "framework_extra_flags": {
        "arduino": "-D__CORTEX_SC=0 -DARDUINO_AWOHALI_STM32L052K8"
