    "frameworks": [
      "arduino",
      "stm32cube",
      "zephyr"
    ],
    "name": "Awohali-Power-Board",
    "upload": {
      "maximum_ram_size": 8192,
      "maximum_size": 65536,
      "protocol": "blackmagic",
      "protocols": [
        "jlink",
